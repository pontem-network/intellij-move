<templateSet group="Move">
    <template name="d"
              value="std::debug::print(&amp;$TEXT$);"
              description="debug print"
              toReformat="false"
              toShortenFQNames="true">
        <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="MOVE_BLOCK" value="true"/>
        </context>
    </template>

    <template name="sd"
              value='std::debug::print(&amp;std::string::utf8(b"$TEXT$"));'
              description="debug print string"
              toReformat="false"
              toShortenFQNames="true">
        <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="MOVE_BLOCK" value="true"/>
        </context>
    </template>

    <template name="wd"
              value='std::debug::print(&amp;aptos_std::string_utils::format1(&amp;b"$VAR$ = {}", $VAR$));'
              description="debug print expression"
              toReformat="false"
              toShortenFQNames="true">
        <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="MOVE_BLOCK" value="true"/>
        </context>
    </template>

    <template name="t"
              value="#[test]&#10;fun $NAME$() {&#10;    $END$&#10;}"
              description="test function"
              toReformat="false">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="MOVE_MODULE" value="true"/>
        </context>
    </template>
</templateSet>
